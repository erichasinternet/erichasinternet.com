{
  "name": "hugo-theme-codex",
  "version": "1.5.0",
  "scripts": {
    "develop": "hugo server -s ./exampleSite/",
    "build:example": "hugo -s ./exampleSite/",
    "format": "prettier ./ --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": ">=10",
    "prettier": "^2.0.5"
  },
  "browserslist": [
    "last 2 version",
    "> 2%"
  ],
  "keywords": [
    "hugo",
    "blog",
    "go"
  ],
  "lint-staged": {
    "*.{js,css,scss}": "prettier --write"
  }
}
